{
  "name": "tsemplate",
  "version": "1.0.0",
  "description": "Online multiplayer party-game web-application",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "start": "npm run clientpp && webpack && node -r ts-node/register src/index.ts",
    "webpack": "webpack --watch",
    "clientpp": "node -r ts-node/register src/utils/rubah.ts",
    "build": "rm -rf build && mkdir build && npm run build:ts && npm run build:ts.d",
    "build:ts": "tsc --project tsconfig.build.json --outDir build",
    "build:ts.d": "tsc -d --project tsconfig.build.json --outDir build --emitDeclarationOnly",
    "test": "mocha",
    "cov": "nyc mocha",
    "tsc": "tsc"
  },
  "author": "Averman",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.8",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "cache-loader": "^4.1.0",
    "css-loader": "^5.0.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.7",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "webpack-cli": "^4.1.0",
    "yargs": "^16.1.1"
  },
  "nyc": {
    "require": [
      "ts-node/register"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "excludeNodeModules": true,
    "instrument": true,
    "sourceMap": true,
    "produce-source-map": true
  },
  "dependencies": {
    "@types/express": "^4.17.8",
    "@types/express-ws": "^3.0.0",
    "@types/image-to-base64": "^2.1.0",
    "@types/react-onsenui": "^2.9.13",
    "@types/serve-favicon": "^2.5.1",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "file-loader": "^6.2.0",
    "image-to-base64": "^2.1.1",
    "onsenui": "^2.11.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hammerjs": "^1.0.1",
    "react-onsenui": "^1.11.2",
    "serve-favicon": "^2.5.0",
    "webpack": "^5.3.1"
  }
}
